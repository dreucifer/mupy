{% from '_utils.j2' import product_table %}

<!DOCTYPE html>

<html lang="en-us"
class="no-js">
<head>
{% block header %}
<meta charset="utf-8">

<title>{{ title }}</title>
<meta name="keywords" content="{{ keywords }}">
<meta name="description" content="{{ description }}">
{% for meta_tag in meta %}
<meta name="{{ meta_tag.id }}" content="{{ meta_tag.content }}">
{% endfor %}
<meta name="GOOGLEBOT" content="INDEX, FOLLOW"><!-- START CUT HERE -->
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="author" href="https://plus.google.com/+alternatorman">
<link href="https://plus.google.com/+alternatorman" rel="publisher">
{% endblock %}
</head>

<body>
