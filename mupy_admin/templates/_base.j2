<![endif]-->
<div id="fb-root"></div>

{% include '_header.j2' %}

<div id="gridbody"
    class="container">
    {% block content %}
        {{ body|safe }}
        {% if product_sets is defined %}
            {% for product_set in product_sets %}
                {% if product_set.products|length > 0 %}
                    {{ product_table(product_set) }}
                {% endif %}
            {% endfor %}
        {% endif %}
    {% endblock %}
</div>

{% include '_footer.j2' %}


